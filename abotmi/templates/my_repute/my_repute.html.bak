    <div class="modal my_repute_page_model"  id="display_adv_compare" tabindex="-1" role="dialog"  aria-labelledby="myModalLabel" aria-hidden="true" style="display:none;">
        <div class="modal-dialog model_display_adv_compare">
          <div id="adv_compare_table"></div>
        </div>
    </div>


    <!-- Pincode Confirmation Popup -->
    <div id="pincode_confirmation_modal" class="modal fade upwardz-modal" role="dialog" name="pincode_confirmation_modal" aria-hidden="true" style="display:none;">
        <div class="modal-dialog modal-md">
            <div class="modal-content">
                <div class="panel upwrdz-log-panel panel-default">
                    <div class="modal-header modal-header-border-bottom text-center">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <i class="fa fa-times-thin fa-2x"></i>
                        </button>
                         <h3 class="modal-title n-span-blue-color">Check your current Pincode</h3>
                        <hr class="modal-heading-hr">
                    </div>
                    <div class="panel-body pannel-popup-content" id="video-request-modal-body">
                        <div class="row padding-tb8">
                            <div class="form-group col-md-12" data-toggle="toggle">
                                <input class="form-control" placeholder="Please enter 4 or 6 digit pincode" name="current_pincode" id="current_pincode" type="text" pattern="^[1-9]\d{3}$|^[1-9]\d{5}$" required='true'>
                                <span class="help-block" ></span>
                            </div>
                            <div class="col-md-6">
                                <button type="button" onclick="transit_hyperlocal_api_call();"  class="btn upwrdz-button" >SUBMIT</button>
                                <button type="button" class="btn upwrdz-button" onclick="cancel_pincode_confirmation_popup();">CANCEL</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
